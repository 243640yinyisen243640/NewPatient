<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="32.5dp"
    android:background="@color/text_white"
    android:orientation="vertical">


    <LinearLayout
        style="@style/hh_fi_wr"
        android:layout_height="32dp">

        <View style="@style/line_home_blood_sugar" />

        <TextView
            android:id="@+id/tv_item_service_blood_seven_time"
            style="@style/text_black_14_w_w"
            android:layout_width="0dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:ellipsize="end"
            android:gravity="center"
            android:text="05-06"
            android:textSize="13sp" />

        <View style="@style/line_home_blood_sugar" />

        <!--凌晨-->

        <FrameLayout
            style="@style/rl_seven_and_thirty_home"
            android:background="@color/background">

            <TextView
                android:id="@+id/tv_item_service_blood_seven_dawn"
                style="@style/tv_seven_and_thirty"
                android:text="123" />


            <ImageView
                android:id="@+id/iv_item_service_blood_seven_dawn"
                style="@style/img_seven_and_thirty_home"
                android:src="@drawable/service_more_data_triangle" />

        </FrameLayout>


        <View style="@style/line_home_blood_sugar" />

        <LinearLayout
            style="@style/hh_wr_wr"
            android:layout_width="0dp"
            android:layout_height="32dp"
            android:layout_weight="2"
            android:orientation="vertical">

            <LinearLayout style="@style/hh_fi_wr">

                <!--早餐前-->
                <FrameLayout
                    style="@style/rl_seven_and_thirty_home">

                    <TextView
                        android:id="@+id/tv_item_service_blood_seven_bre_emp"
                        style="@style/tv_seven_and_thirty"
                        android:text="123" />


                    <ImageView
                        android:id="@+id/iv_item_service_blood_seven_bre_emp"
                        style="@style/img_seven_and_thirty_home"
                        android:src="@drawable/service_more_data_triangle" />

                </FrameLayout>


                <View style="@style/line_home_blood_sugar" />
                <!--早餐后-->


                <FrameLayout
                    style="@style/rl_seven_and_thirty_home">

                    <TextView
                        android:id="@+id/tv_item_service_blood_seven_bre_later"
                        style="@style/tv_seven_and_thirty"
                        android:text="123" />


                    <ImageView
                        android:id="@+id/iv_item_service_blood_seven_bre_later"
                        style="@style/img_seven_and_thirty_home"
                        android:src="@drawable/service_more_data_triangle" />

                </FrameLayout>


            </LinearLayout>


        </LinearLayout>


        <View style="@style/line_home_blood_sugar" />


        <LinearLayout
            style="@style/hh_wr_wr"
            android:layout_width="0dp"
            android:layout_height="32dp"
            android:layout_weight="2"
            android:orientation="vertical">

            <LinearLayout style="@style/hh_fi_wr">

                <!--午餐前-->
                <FrameLayout
                    style="@style/rl_seven_and_thirty_home"
                    android:background="@color/background">
                    <TextView
                        android:id="@+id/tv_item_service_blood_seven_lunch_before"
                        style="@style/tv_seven_and_thirty"
                        android:text="123" />


                    <ImageView
                        android:id="@+id/iv_item_service_blood_seven_lunch_before"
                        style="@style/img_seven_and_thirty_home"
                        android:src="@drawable/service_more_data_triangle" />

                </FrameLayout>


                <View style="@style/line_home_blood_sugar" />
                <!--午餐后-->
                <FrameLayout
                    style="@style/rl_seven_and_thirty_home"
                    android:background="@color/background">

                    <TextView
                        android:id="@+id/tv_item_service_blood_seven_lunch_later"
                        style="@style/tv_seven_and_thirty"
                        android:text="123" />


                    <ImageView
                        android:id="@+id/iv_item_service_blood_seven_lunch_later"
                        style="@style/img_seven_and_thirty_home"
                        android:src="@drawable/service_more_data_triangle" />

                </FrameLayout>


            </LinearLayout>


        </LinearLayout>


        <View style="@style/line_home_blood_sugar" />


        <LinearLayout
            style="@style/hh_wr_wr"
            android:layout_width="0dp"
            android:layout_height="32dp"
            android:layout_weight="2"
            android:orientation="vertical">

            <LinearLayout style="@style/hh_fi_wr">

                <!--晚餐前-->
                <FrameLayout
                    style="@style/rl_seven_and_thirty_home">

                    <TextView
                        android:id="@+id/tv_item_service_blood_seven_dinner_before"
                        style="@style/tv_seven_and_thirty"
                        android:text="123" />


                    <ImageView
                        android:id="@+id/iv_item_service_blood_seven_dinner_before"
                        style="@style/img_seven_and_thirty_home"
                        android:src="@drawable/service_more_data_triangle" />

                </FrameLayout>


                <View style="@style/line_home_blood_sugar" />
                <!--晚餐后-->

                <FrameLayout
                    style="@style/rl_seven_and_thirty_home">

                    <TextView
                        android:id="@+id/tv_item_service_blood_seven_dinner_later"
                        style="@style/tv_seven_and_thirty"
                        android:text="123" />


                    <ImageView
                        android:id="@+id/iv_item_service_blood_seven_dinner_later"
                        style="@style/img_seven_and_thirty_home"
                        android:src="@drawable/service_more_data_triangle" />

                </FrameLayout>


            </LinearLayout>


        </LinearLayout>


        <View style="@style/line_home_blood_sugar" />
        <!--睡前-->
        <FrameLayout
            style="@style/rl_seven_and_thirty_home"
            android:background="@color/background">

            <TextView
                android:id="@+id/tv_item_service_blood_seven_bed_time"
                style="@style/tv_seven_and_thirty"
                android:text="123" />


            <ImageView
                android:id="@+id/iv_item_service_blood_seven_bed_time"
                style="@style/img_seven_and_thirty_home"
                android:src="@drawable/service_more_data_triangle" />

        </FrameLayout>


        <View style="@style/line_home_blood_sugar" />
    </LinearLayout>


    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/background" />
</LinearLayout>


